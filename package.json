{
  "name": "andreas-mausch-de",
  "version": "1.0.0",
  "scripts": {
    "dev": "ELEVENTY_ENV=development eleventy --serve",
    "build": "ELEVENTY_ENV=production eleventy",
    "generate:font": "mkdirp ./_site/styles/icons/ && iconism --output ./_site/styles/icons/ --types ttf,woff2 --assets css --prefix icon --debug ./icon-font.json",
    "lint": "npm run lint:scripts && npm run lint:scss",
    "lint:scripts": "eslint .",
    "lint:scss": "stylelint ./styles/**/*.scss",
    "check:types": "tsc",
    "check:links": "linkinator --skip=linkedin.com,github.com --recurse --timeout 3000 --verbosity error ./_site/",
    "clean": "rimraf ./_site/",
    "postinstall": "download https://github.com/plantuml/plantuml/releases/download/v1.2024.6/plantuml-1.2024.6.jar --out . --filename plantuml.jar"
  },
  "license": "MIT",
  "dependencies": {
    "@fontsource/raleway": "5.0.19",
    "img-comparison-slider": "8.0.6",
    "katex": "0.16.10",
    "prism-themes": "1.9.0",
    "swiffy-slider": "1.6.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-img": "4.0.2",
    "@11ty/eleventy-plugin-directory-output": "1.0.1",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@fortawesome/fontawesome-free": "6.5.2",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "autoprefixer": "10.4.19",
    "download-cli": "1.1.1",
    "eleventy-plugin-emoji": "1.1.0",
    "eleventy-plugin-nesting-toc": "1.3.0",
    "eleventy-sass": "2.2.4",
    "esbuild": "0.21.1",
    "eslint": "8.56.0",
    "glob": "10.3.14",
    "iconism": "1.2.3",
    "linkinator": "6.0.4",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "8.6.7",
    "markdown-it-attribution": "0.1.4",
    "markdown-it-attrs": "4.1.6",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-footnote": "4.0.0",
    "markdown-it-plantuml-ex2": "1.0.1",
    "markdown-it-prism": "2.3.0",
    "markdown-it-task-checkbox": "1.0.6",
    "mkdirp": "3.0.1",
    "postcss": "8.4.38",
    "pre-commit": "1.2.2",
    "qrcode": "1.5.3",
    "rimraf": "5.0.5",
    "stylelint": "15.4.0",
    "stylelint-config-sass-guidelines": "10.0.0",
    "typescript": "5.4.5"
  },
  "pre-commit": [
    "lint",
    "check:types"
  ]
}
