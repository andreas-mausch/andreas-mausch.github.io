{
  "name": "andreas-mausch-de",
  "version": "1.0.0",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "ELEVENTY_ENV=production eleventy",
    "generate:font": "icon-font-buildr --config ../../../../icon-font.json",
    "lint": "npm run lint:scripts && npm run lint:scss",
    "lint:scripts": "eslint --ignore-pattern !.eleventy.js ./.eleventy.js ./eleventy/**/*.js ./scripts/**/*.ts",
    "lint:scss": "stylelint ./styles/**/*.scss",
    "check": "tsc",
    "clean": "rimraf ./_site/"
  },
  "license": "MIT",
  "dependencies": {
    "@fontsource/raleway": "4.5.10",
    "img-comparison-slider": "7.7.2",
    "katex": "0.16.2",
    "prism-themes": "1.9.0",
    "swiffy-slider": "1.5.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.0-canary.14",
    "@11ty/eleventy-img": "2.0.1",
    "@11ty/eleventy-plugin-directory-output": "1.0.1",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@fortawesome/fontawesome-free": "6.2.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "10.4.9",
    "eleventy-plugin-emoji": "1.1.0",
    "eleventy-plugin-toc": "1.1.5",
    "eleventy-sass": "2.1.2",
    "esbuild": "0.15.7",
    "eslint": "8.23.1",
    "glob": "8.0.3",
    "icon-font-buildr": "1.4.2",
    "markdown-it-anchor": "8.6.5",
    "markdown-it-attribution": "0.1.4",
    "markdown-it-attrs": "4.1.4",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-footnote": "3.0.3",
    "markdown-it-prism": "2.3.0",
    "markdown-it-task-checkbox": "1.0.6",
    "postcss": "8.4.16",
    "pre-commit": "1.2.2",
    "rimraf": "3.0.2",
    "stylelint": "14.11.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "typescript": "4.8.3"
  },
  "pre-commit": [
    "lint",
    "check"
  ]
}
