{
  "name": "eleventy-sample",
  "version": "1.0.0",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "ELEVENTY_ENV=production eleventy",
    "lint": "npm run lint:scripts && npm run lint:scss",
    "lint:scripts": "eslint --ignore-pattern !.eleventy.js ./.eleventy.js ./eleventy/**/*.js ./scripts/**/*.ts",
    "lint:scss": "stylelint ./styles/**/*.scss",
    "check": "tsc",
    "clean": "rimraf ./_site/"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "2.0.0-canary.14",
    "@11ty/eleventy-img": "2.0.1",
    "@11ty/eleventy-plugin-directory-output": "1.0.1",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@types/toastify-js": "1.11.1",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "autoprefixer": "10.4.8",
    "eleventy-plugin-emoji": "1.1.0",
    "eleventy-plugin-toc": "1.1.5",
    "eleventy-sass": "2.1.0",
    "esbuild": "0.15.1",
    "eslint": "8.21.0",
    "glob": "8.0.3",
    "markdown-it-anchor": "8.6.4",
    "markdown-it-attribution": "0.1.4",
    "markdown-it-attrs": "4.1.4",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-footnote": "3.0.3",
    "markdown-it-prism": "2.3.0",
    "markdown-it-task-checkbox": "1.0.6",
    "postcss": "8.4.20",
    "pre-commit": "1.2.2",
    "rimraf": "3.0.2",
    "stylelint": "14.10.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "img-comparison-slider": "7.7.2",
    "katex": "0.16.0",
    "prism-themes": "1.9.0",
    "swiffy-slider": "1.5.3",
    "toastify-js": "1.12.0"
  },
  "pre-commit": [
    "lint",
    "check"
  ]
}
